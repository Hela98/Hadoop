# Edit this file by following the given format:
# =============================================
#
# [group-name] => master or workers
# <public-ip-address>   hostname=<arbitrary-hostname> internal_ip=<internal-ip>
# 
# Example:
[all]
hadoop-master   ansible_host=172.174.172.201 internal_ip=10.0.0.5 ansible_user=ansible_user   ansible_ssh_pass=OneWorld9816* ansible_connection=ssh
hadoop-node1   ansible_host=172.173.136.178 internal_ip=10.0.0.6 ansible_user=ansible_user    ansible_ssh_pass=OneWorld9816* ansible_connection=ssh
hadoop-node2   ansible_host=40.117.181.28 internal_ip=10.0.0.7 ansible_user=ansible_user    ansible_ssh_pass=OneWorld9816* ansible_connection=ssh

[masters]
hadoop-master   ansible_host=172.174.172.201 internal_ip=10.0.0.5 ansible_user=ansible_user   ansible_ssh_pass=OneWorld9816* ansible_connection=ssh

[workers]
hadoop-node1   ansible_host=172.173.136.178 internal_ip=10.0.0.6 ansible_user=ansible_user    ansible_ssh_pass=OneWorld9816* ansible_connection=ssh
hadoop-node2   ansible_host=40.117.181.28 internal_ip=10.0.0.7 ansible_user=ansible_user    ansible_ssh_pass=OneWorld9816* ansible_connection=ssh


